-- Scripts:

local function GKAVNBC_fake_script() -- Main.Smooth GUI Dragging 
	local script = Instance.new('LocalScript', Main)

	local UserInputService = game:GetService("UserInputService")
	local runService = (game:GetService("RunService"));
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	function Lerp(a, b, m)
		return a + (b - a) * m
	end;
	
	local lastMousePos
	local lastGoalPos
	local DRAG_SPEED = (8); -- // The speed of the UI darg.
	function Update(dt)
		if not (startPos) then return end;
		if not (dragging) and (lastGoalPos) then
			gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, lastGoalPos.X.Offset, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, lastGoalPos.Y.Offset, dt * DRAG_SPEED))
			return 
		end;
	
		local delta = (lastMousePos - UserInputService:GetMouseLocation())
		local xGoal = (startPos.X.Offset - delta.X);
		local yGoal = (startPos.Y.Offset - delta.Y);
		lastGoalPos = UDim2.new(startPos.X.Scale, xGoal, startPos.Y.Scale, yGoal)
		gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, xGoal, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, yGoal, dt * DRAG_SPEED))
	end;
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			lastMousePos = UserInputService:GetMouseLocation()
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	runService.Heartbeat:Connect(Update)
end
coroutine.wrap(GKAVNBC_fake_script)()
local function SJEGFX_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local Close = script.Parent.Main.Close
	local Mods = script.Parent.Main
	local start = script.Parent.Startup
	local OC = script.Parent.Open
	local Openc =script.Parent.Open.Open
	local Die = script.Parent.Main.die
	local core = script.Parent.Parent.ScreenGui
	Mods.Draggable=true
	Mods.Active=true
	--Buttons--
	
	local Rows = Mods.ScrollingFrame
	local Mitch = Rows.Row1
	local Ebg = Rows.Row1.ElementalBg
	local Zrush = Rows.Row2
	local Ohub = Rows.Row2.Owlh
	local ProL =Rows.Row3
	local Bab =Rows.Row3.Bab
	local Lt =Rows.Row4
	local Jail = Rows.Row4.Jail
	local Madc = Rows.Row5
	local Madcu = Rows.Row5.Jail
	----Button Functions----
	Zrush.MouseButton1Down:connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/SlamminPig/rblxgames/main/Zombie%20Rush/ZombieRushGUI"))()
	end)
	Mitch.MouseButton1Down:connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/CodexGaming09/Scripts/main/MZomb?token=GHSAT0AAAAAABQR4JS27SINISS4RM2EBJQQYPUX7FA"))()
	end)
	Ebg.MouseButton1Down:connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/CodexGaming09/Scripts/main/ElementalBG"))()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/CodexGaming09/Scripts/main/Ebg2"))()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/CodexGaming09/Scripts/main/MM2"))()
	end)
	Ohub.MouseButton1Down:connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/ZinityDrops/OwlHubLink/master/OwlHubBack.lua"))();
	end)
	ProL.MouseButton1Down:connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/CodexGaming09/Scripts/main/ProjectL"))();
	end)
	Bab.MouseButton1Down:connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/CodexGaming09/Scripts/main/BuildABoat"))();
	end)
	Lt.MouseButton1Down:connect(function()
		loadstring(game:HttpGetAsync'https://cdn.applebee1558.com/bark/bark.lua')('2022 more like twenty-twenty-too')
	end)
	Jail.MouseButton1Down:connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/CodexGaming09/Scripts/main/JailBreak"))();
	end)
	Madcu.MouseButton1Down:connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/CodexGaming09/Scripts/main/MadcityCuff"))();
	end)
	Madc.MouseButton1Down:connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/CodexGaming09/Scripts/main/mADCITY"))();
	end)
	----Dont Edit Here----
	Mods.Visible=false
	start.Visible=true
	OC.Visible = false
	start.Visible=false
	OC.Visible=true
	
	Openc.MouseButton1Down:connect(function()
		OC.Visible = false
	Mods.Visible=true
		Mods:TweenPosition(UDim2.new({0.274, 0},{0.297, 0}), 'Out', 'Bounce', '3')
	end)
	Die.MouseButton1Down:connect(function()
		core:Destroy()
	end)
	Close.MouseButton1Down:connect(function()
		Mods:TweenPosition(UDim2.new(0, 0,2, 0), 'Out', 'Bounce', '3')
	wait(2)
		Mods.Visible = false
		OC.Visible=true
	end)
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
end
coroutine.wrap(SJEGFX_fake_script)()
